{
  "name": "templatecli",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --port=8083",
    "android:dev": "react-native run-android --mode=DevelopmentDebug --appIdSuffix=dev --port=8083",
    "android:dev:release": "react-native run-android --mode=DevelopmentRelease --appIdSuffix=dev --port=8083",
    "android:dev-apk": "cd android && ./gradlew assembleDevelopmentRelease",
    "android:dev-aab": "cd android && ./gradlew bundleDevelopmentRelease",
    "android:prod": "react-native run-android --mode=ProductionDebug --port=8083",
    "android:prod:release": "react-native run-android --mode=ProductionRelease --port=8083",
    "android:prod-apk": "cd android && ./gradlew assembleProductionRelease",
    "android:prod-aab": "cd android && ./gradlew bundleProductionRelease",
    "ios:dev": "react-native run-ios --scheme DEV --mode DevelopmentDebug --port=8083",
    "ios:dev:release": "react-native run-ios --scheme DEV --mode DevelopmentRelease --port=8083",
    "ios:prod": "react-native run-ios --scheme PROD --mode ProductionDebug --port=8083",
    "ios:prod:release": "react-native run-ios --scheme PROD --mode ProductionRelease --port=8083",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@logicwind/react-native-exit-app": "^0.2.0",
    "@react-native-community/hooks": "^100.1.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.10.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jotai": "^2.12.5",
    "patch-package": "^8.0.0",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-config": "1.5.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-worklets": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
